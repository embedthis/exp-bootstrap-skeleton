{
    "name": "exp-bootstrap-skeleton",
    "description": "Bootstrap Skeleton for Expansive",
    "version": "1.0.0",
    "keywords": [
        "expansive-plugin",
        "bootstrap"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-bootstrap-skeleton.git"
    },
    "dependencies": {
        "bootstrap": "~3.3",
        "embedthis/exp-js": "~1",
        "embedthis/exp-less": "~1",
        "embedthis/exp-css": "~1",
        "embedthis/exp-html": "~1",
        "embedthis/exp-markdown": "~1",
        "embedthis/exp-canon": "~1"
    },
    "devDependencies": {
        "expansive": ">=0.7.0",
        "pak": ">=0.11.3"
    },
    "blend": {
        "devDependencies": {
            "?esp": ">=7.0"
        },
        "directories": {
            "?export": "contents/lib"
        },
        "?profile": "debug",
        "?import": true
    },
    "export": {
        "from": [
            "contents/**",
            "layouts/**",
            "partials/**"
        ],
        "to": "${TOP}",
        "overwrite": false
    },
    "manage": "expansive.json"
}
