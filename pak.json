{
    "name": "exp-bootstrap-skeleton",
    "description": "Bootstrap Skeleton for Expansive",
    "version": "0.2.1",
    "keywords": [
        "expansive-plugin",
        "bootstrap"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-bootstrap-skeleton.git"
    },
    "dependencies": {
        "bootstrap": "~3.3",
        "exp-js": "~0.3",
        "exp-less": "~0.3",
        "exp-css": "~0.3",
        "exp-html": "~0.2",
        "exp-markdown": "~0.3",
        "exp-canon": "~0.1"
    },
    "devDependencies": {
        "expansive": ">=0.7.0",
        "pak": ">=0.11.3"
    },
    "blend": {
        "devDependencies": {
            "?esp": ">=7.0"
        },
        "directories": {
            "?export": "contents/lib"
        },
        "?profile": "debug",
        "?import": true
    },
    "export": {
        "from": [
            "contents/**",
            "layouts/**",
            "partials/**"
        ],
        "to": "${TOP}",
        "overwrite": false
    },
    "manage": "expansive.json"
}
